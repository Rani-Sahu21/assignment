#include<bits/stdc++.h>
using namespace std;
int main(){
    int a[100],b[100],n,c[100];
    cout<<"enter the number of elements"<<endl;
    cin>>n;
    cout<<"enter the  elements of 1 array "<<endl;
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    cout<<"enter the  elements of 2 array "<<endl;
    for(int i=0;i<n;i++){
        cin>>b[i];
    }
    
    for(int i=0;i<n;i++){
        c[i]=a[i]+b[i];
    }
    cout<<"sum of slements of corresponding array"<<endl;
    for(int i=0;i<n;i++){
        cout<<c[i]<<endl;
        cout<<"";
    }
    return 0;
}
