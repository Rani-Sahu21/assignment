#include<iostream>
#include<vector>
using namespace std;
int main(){
    int a;
    vector<int>v;
    for(int i=0;i<5;i++){
   cin>>a;
   v.push_back(a);
    }

    cout<<"capacity "<<v.capacity()<<endl;
    cout<<"size  "<<v.size()<<endl;

    float sum=0;

     for(int i=0;i<5;i++){
            sum+=v[i];
          }

          float p=(sum*100)/500;

          if(p>90&&p<=100)
          cout<<"Grade A";
          if(p>80&&p<=90)
          cout<<"Grade B";
          if(p>60&&p<=80)
          cout<<"Grade C";
          if(p>0&&p<=60)
          cout<<"Grade D";
    
    return 0;
}
