#include<bits/stdc++.h>
using namespace std;
int main(){
    vector<int>v;
    int n,a;
    cout<<"enter the numbers"<<endl;
    cin>>n;
    for(int i=0;i<n;i++ ){
        cin>>a;
        v.push_back(a);

    }
         int sum=0;
    for(int i=0;i<n;i++){
          sum+=v[i];
    }

    cout<<"sum of all the no. are "<<sum;
    return 0;
}
