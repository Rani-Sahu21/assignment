#include<bits/stdc++.h>
using namespace std;
int main(){
    //greatest of the three number

    int a,b,c;
    cout<<"enter the three no."<<endl;
    cin>>a>>b>>c;
    if(a>b){
        if(a>c){
            cout<<"a is the greatest no."<<a;
        }
    }

    else if(b>a){
        if(b>c){
            cout<<"b is the greatest no."<<b;
        }
    }
    else if(c>a){
        if(c>b){
            cout<<"c is the greatest no."<<c;
        }
    }
    return 0;
}
