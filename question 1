   int a;
   vector<int>v;

  

   
      v.push_back(95);
      v.push_back(87);
      v.push_back(94);
      v.push_back(87);
      v.push_back(85);
      v.push_back(91);
   
     cout<<"enter the marks respectively "<<endl;
    cout<<"Mathematics"<<endl;
