#include <bits/stdc++.h>
using namespace std;

int main(){
    vector<int>array;
    int n =0 ;
    cout<<"Enter the size of the array: "<<endl;
    cin>>n ;

    for(int i =0 ; i< n ; i++){
        int x; 
        cin>>x;
        array.push_back(x);
    }
    int sum =0 ;
    for(int i =0 ; i< n ; i++){
        sum = sum + array[i];
    }
    cout<<sum;

    return 0;
}
